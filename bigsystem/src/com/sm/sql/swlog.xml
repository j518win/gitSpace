<sqlMap namespace="swlog">		<insert id="writelog" parameterClass="java.util.HashMap">		 insert into SOS_SYS_LOG (			 LOGIN_ID,			 LOGIN_IP,			 LOGIN_MAC,			 OP_NUM,			 OP_FUNC,			 OP_DATE,			 OP_NOTE		 )values (		 	#loginId#,		 	#ipAddr#,		 	#macAddr#,		 	#num#,		 	#func#,		 	sysdate(),		 	#makeNote#		 	)	</insert>	</sqlMap>